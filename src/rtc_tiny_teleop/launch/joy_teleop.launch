<?xml version="1.0"?>
<launch>
 <!-- joy node -->
  <node respawn="true" pkg="joy"
        type="joy_node" name="joy" >
    <param name="dev" type="string" value="/dev/input/js0" />
    <param name="autorepeat_rate" value="10" />
  </node>

  <node pkg="rtc_tiny_teleop" type="joy_teleop" name="joy_teleop" output="screen"/>

  <!--node pkg="rtc_tiny_teleop" type="rtc_tiny_controller" name="rtc_tiny_controller" output="screen"/-->
</launch>